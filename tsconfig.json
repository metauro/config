{
  "compilerOptions": {
    "module": "umd",
    "moduleResolution": "node",
    "noLib": false,
    "declaration": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "sourceMap": false,
    "allowJs": false,
    "baseUrl": "./",
    "lib": ["es2017"]
  }
}
